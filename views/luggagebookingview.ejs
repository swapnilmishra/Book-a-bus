<% include header %>
	<div><h3 class="text-center text-muted">New Luggage Booking</h3></div>
	<div class="row">
		<div class="col-md-4 col-md-offset-4 booking-form">
			<form role="form" method="POST" action="/luggage/dobooking" id="luggageBookingForm">
			  <div class="form-group">
			    <label>Delivery From</label>
			    <input class="form-control" name="from_name" placeholder="Enter name">
			  </div>
			  <div class="form-group">
			    <label>Delivery to</label>
			    <input class="form-control" name="to_name" placeholder="Enter name">
			  </div>
			  <div class="form-group">
			    <label>Station</label>
			    <input class="form-control" name="station" placeholder="Delivery place name">
			  </div>
			  <div class="form-group">
			    <label>Parcel</label>
			    <input class="form-control" name="parcel" placeholder="Parcel name">
			  </div>
			  <div class="form-group">
			    <label>Parcel Type</label>
			    <select class="form-control" name="parcel_type" placeholder="Parcel type">
			    	<option>Roll</option>
			    	<option>Document</option>
			    	<option>Carton</option>
			    	<option>Jhaal</option>
			    	<option>Bundle</option>
			    </select>
			  </div>
			  <div class="form-group">
			    <label>Transport Charges</label>
			    <input class="form-control" name="transporting_charges" placeholder="Rs">
			  </div>
			  <button type="submit" class="btn btn-primary pull-right">BOOK</button>
			</form>
		</div>
	</div>
<% include footer %>