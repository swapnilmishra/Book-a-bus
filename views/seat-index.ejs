<% include header %>
	<div class="row">
		<div class="col-md-6 col-md-offset-3">
			<form class="form-date form-inline" method="POST" action="/seat/booking">
				<h3 class="text-muted">New seat booking</h3>
		    <input type="text" class="form-control input-lg" placeholder="Journey Date" name="journeydate">
		    <select class="form-control input-lg" name="busnumber">
		    	<% for(var i=0; i < busNumbers.length; i++) { %>
		    		<option value="<%=busNumbers[i]%>"><%=busNumbers[i]%></option>
		    	<% } %>
		    </select>
		    <button type="submit" class="btn btn-default btn-lg btn-primary btn-block">New Booking</button>
		  </form>
		</div>
	</div>
<% include footer %>