<div class="row">
	<div class="col-md-2">
		<span class="glyphicon glyphicon-ban-circle icon-driver"></span>
	</div>
	<div class="col-md-9">
		<% for( var i=3; i > 0; i--) { %>
			<ul class="list-inline seats">
	  		<% for( var j=i; j < (i+9*3) ; j=j+3) { %>
	  			<%if(bookedSeats && bookedSeats.indexOf(j.toString()) != -1 ) { %>
	  				<li class="seat seat-booked" data-seat-name="<%=j%>">
		  				<a href="#" data-toggle="tooltip" title="" data-original-title="<%=j%>"><span class="glyphicon glyphicon-compressed"></span></a>
		  			</li>
	  			<% } else { %>
		  			<li class="seat" data-seat-name="<%=j%>">
		  				<a href="#" data-toggle="tooltip" title="" data-original-title="<%=j%>"><span class="glyphicon glyphicon-compressed"></span></a>
		  			</li>
	  			<% } %>
	    	<% } %>
	  	</ul>	
		<% } %>
  	<span class="glyphicon glyphicon-arrow-left arrow-driver"> Driver</span>
	</div>
	<div class="col-md-1">
		<ul class="list-unstyled seats-back">
			<% for( var j=5; j >0; j--) { %>
	  		<%if(bookedSeats && bookedSeats.indexOf((j+27).toString()) != -1 ) { %>
	  			<li class="seat seat-booked" data-seat-name="<%=j+27%>">
	  				<a href="#" data-toggle="tooltip" title="" data-original-title="<%=j+27%>"><span class="glyphicon glyphicon-compressed"></span></a>
	  			</li>
	  		<% } else { %>
	  			<li class="seat" data-seat-name="<%=j+27%>">
	  				<a href="#" data-toggle="tooltip" title="" data-original-title="<%=j+27%>"><span class="glyphicon glyphicon-compressed"></span></a>
	  			</li>
  			<% } %>
	    <% } %>
		</ul>
	</div>
</div>