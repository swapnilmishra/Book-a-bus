<div class="row">
	<div class="col-md-2">
		<span class="glyphicon glyphicon-ban-circle icon-driver"></span>
	</div>
	<div class="col-md-10">
		<% for( var i=0; i < seatTypes.length; i++) { %>
			<ul class="list-inline seats">
	  		<% for( var j=0,seats=sleeperSeats[seatTypes[i]]; j < sleeperSeats[seatTypes[i]].length; j++) { %>
	  			<%if(bookedSeats && bookedSeats.indexOf(seats[j]) !== -1 ) { %>
	  				<li class="seat sleeper seat-booked" data-seat-name="<%=seats[j]%>">
		  				<a href="#" data-toggle="tooltip" title="" data-original-title="<%=seats[j]%>">
		  					<span class="glyphicon glyphicon-compressed"></span>
		  					<span style="color:#fff;font-weight:bold">
		  						<%if(femaleSeats && femaleSeats.indexOf(seats[j]) !== -1 ) { %>
		  							 &nbsp; F
		  						<% } %>
		  					</span>
		  				</a>
		  			</li>
	  			<% } else { %>
		  			<li class="seat sleeper" data-seat-name="<%=seats[j]%>">
		  				<a href="#" data-toggle="tooltip" title="" data-original-title="<%=seats[j]%>"><span class="glyphicon glyphicon-compressed"></span></a>
		  			</li>
	  			<% } %>
	    	<% } %>
	  	</ul>	
		<% } %>
  	<span class="glyphicon glyphicon-arrow-left arrow-driver"> Driver</span>

	</div>
</div>